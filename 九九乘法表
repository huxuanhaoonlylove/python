for i in range(1,10):
    for j in range(1,i+1):
        print('%1d*%1d=%2d'%(i,j,i*j),end=' ')
    print()

for i in range(1,10):
    for j in range(i,10):
        print('%1d*%1d=%2d'%(i,j,i*j),end=' ')
    print()

for i in range(1,10):
    for j in range(1,i+1):
        print('%1d*%1d=%2d'%(j,i,i*j),end=' ')
    print()

i = 9    
while i >= 1:
    for j in range(i,10):
        print('%1d*%1d=%2d'%(i,j,i*j),end=' ')
    i = i - 1
    print()
